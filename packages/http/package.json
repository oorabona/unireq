{
  "name": "@unireq/http",
  "version": "0.0.1",
  "description": "HTTP(S) transport and policies for unireq using Node's built-in fetch (undici)",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "keywords": [
    "http",
    "https",
    "fetch",
    "undici",
    "transport"
  ],
  "author": "Olivier Orabona",
  "license": "MIT",
  "dependencies": {
    "@unireq/core": "workspace:*",
    "@unireq/config": "workspace:*"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "tsup": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oorabona/unireq",
    "directory": "packages/http"
  },
  "bugs": {
    "url": "https://github.com/oorabona/unireq/issues"
  },
  "homepage": "https://github.com/oorabona/unireq/tree/main/packages/http"
}
