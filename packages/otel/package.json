{
  "name": "@unireq/otel",
  "version": "0.0.1",
  "description": "OpenTelemetry instrumentation for unireq HTTP clients",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "keywords": [
    "opentelemetry",
    "otel",
    "tracing",
    "observability",
    "instrumentation"
  ],
  "author": "Olivier Orabona",
  "license": "MIT",
  "dependencies": {
    "@unireq/core": "workspace:*"
  },
  "peerDependencies": {
    "@opentelemetry/api": "catalog:",
    "@opentelemetry/semantic-conventions": "catalog:"
  },
  "devDependencies": {
    "@opentelemetry/api": "catalog:",
    "@opentelemetry/semantic-conventions": "catalog:",
    "typescript": "catalog:",
    "tsup": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oorabona/unireq",
    "directory": "packages/otel"
  },
  "bugs": {
    "url": "https://github.com/oorabona/unireq/issues"
  },
  "homepage": "https://github.com/oorabona/unireq/tree/main/packages/otel"
}
