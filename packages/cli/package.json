{
  "name": "@unireq/cli",
  "version": "0.0.1",
  "description": "HTTP CLI client with REPL mode for API exploration and testing",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "unireq": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/cli.ts",
    "dev:repl": "tsx src/cli.ts repl",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist *.tsbuildinfo",
    "generate:commands": "tsx scripts/generate-commands-doc.ts",
    "validate:commands": "tsx scripts/validate-commands.ts"
  },
  "keywords": [
    "cli",
    "http",
    "api",
    "repl",
    "rest",
    "client",
    "curl",
    "httpie"
  ],
  "author": "Olivier Orabona",
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "catalog:",
    "@inkjs/ui": "catalog:",
    "@node-rs/argon2": "catalog:",
    "@scalar/openapi-parser": "catalog:",
    "@unireq/config": "workspace:*",
    "@unireq/core": "workspace:*",
    "@unireq/http": "workspace:*",
    "@unireq/presets": "workspace:*",
    "citty": "catalog:",
    "consola": "catalog:",
    "fast-xml-parser": "catalog:",
    "ink": "catalog:",
    "ink-scroll-view": "^0.3.5",
    "picocolors": "catalog:",
    "react": "catalog:",
    "valibot": "catalog:",
    "yaml": "catalog:"
  },
  "optionalDependencies": {
    "@napi-rs/keyring": "catalog:"
  },
  "devDependencies": {
    "@types/react": "catalog:",
    "execa": "catalog:",
    "ink-testing-library": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "undici": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oorabona/unireq",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/oorabona/unireq/issues"
  },
  "homepage": "https://github.com/oorabona/unireq/tree/main/packages/cli"
}
