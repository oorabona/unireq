{
  "name": "@unireq/monorepo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Pipe-first, tree-shakeable, multi-protocol I/O toolkit",
  "keywords": [
    "http",
    "http2",
    "fetch",
    "undici",
    "imap",
    "ftp",
    "oauth",
    "rest",
    "api",
    "client"
  ],
  "author": "Olivier Orabona",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oorabona/unireq"
  },
  "bugs": {
    "url": "https://github.com/oorabona/unireq/issues"
  },
  "homepage": "https://github.com/oorabona/unireq#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "cli": "pnpm --filter @unireq/cli exec node dist/cli.js",
    "build": "pnpm -r run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run tests/integration",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "lint:examples": "biome check examples/",
    "lint:examples:fix": "biome check --write examples/",
    "format": "biome format --write .",
    "type-check": "pnpm -r run type-check && tsc --project tests/integration/tsconfig.json --noEmit",
    "type-check:examples": "tsc --project examples/tsconfig.json --noEmit",
    "clean": "pnpm -r run clean && rm -rf node_modules",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && pnpm changeset publish",
    "mock-server": "tsx scripts/mock-server/server.ts",
    "example": "tsx --tsconfig examples/tsconfig.json",
    "examples:all": "bash scripts/run-all-examples.sh",
    "examples:run": "bash scripts/run-examples.sh",
    "example:http": "tsx --tsconfig examples/tsconfig.json examples/http-basic.ts",
    "example:oauth": "tsx --tsconfig examples/tsconfig.json examples/oauth-refresh.ts",
    "example:retry": "tsx --tsconfig examples/tsconfig.json examples/retry-backoff.ts",
    "example:multipart": "tsx --tsconfig examples/tsconfig.json examples/multipart-upload.ts",
    "example:bulk-upload": "tsx --tsconfig examples/tsconfig.json examples/bulk-document-upload.ts",
    "example:ecommerce": "tsx --tsconfig examples/tsconfig.json examples/ecommerce-product-create.ts",
    "example:ci-artifacts": "tsx --tsconfig examples/tsconfig.json examples/ci-artifacts-upload.ts",
    "example:email": "tsx --tsconfig examples/tsconfig.json examples/email-with-attachments.ts",
    "example:form": "tsx --tsconfig examples/tsconfig.json examples/form-submission.ts",
    "example:graphql-query": "tsx --tsconfig examples/tsconfig.json examples/graphql-query.ts",
    "example:graphql-mutation": "tsx --tsconfig examples/tsconfig.json examples/graphql-mutation.ts",
    "example:streaming-upload": "tsx --tsconfig examples/tsconfig.json examples/streaming-upload.ts",
    "example:streaming-download": "tsx --tsconfig examples/tsconfig.json examples/streaming-download.ts",
    "example:sse": "tsx --tsconfig examples/tsconfig.json examples/sse-events.ts",
    "example:interceptors-logging": "tsx --tsconfig examples/tsconfig.json examples/interceptors-logging.ts",
    "example:http-verbs": "tsx --tsconfig examples/tsconfig.json examples/http-verbs.ts",
    "example:interceptors-metrics": "tsx --tsconfig examples/tsconfig.json examples/interceptors-metrics.ts",
    "example:interceptors-cache": "tsx --tsconfig examples/tsconfig.json examples/interceptors-cache.ts",
    "example:conditional-etag": "tsx --tsconfig examples/tsconfig.json examples/conditional-etag.ts",
    "example:conditional-lastmodified": "tsx --tsconfig examples/tsconfig.json examples/conditional-lastmodified.ts",
    "example:conditional-combined": "tsx --tsconfig examples/tsconfig.json examples/conditional-combined.ts",
    "example:inspect": "tsx --tsconfig examples/tsconfig.json examples/inspect-api.ts",
    "example:validation": "tsx --tsconfig examples/tsconfig.json examples/validation-demo.ts",
    "example:logging-standard": "tsx --tsconfig examples/tsconfig.json examples/logging-standard.ts",
    "example:logging-pino": "tsx --tsconfig examples/tsconfig.json examples/logging-pino.ts",
    "example:error-handling": "tsx --tsconfig examples/tsconfig.json examples/error-handling.ts",
    "example:preset-builder": "tsx --tsconfig examples/tsconfig.json examples/preset-builder.ts",
    "example:imap": "tsx --tsconfig examples/tsconfig.json examples/imap-inbox-reader.ts",
    "example:ftp": "tsx --tsconfig examples/tsconfig.json examples/ftp-backup-sync.ts",
    "example:h2": "tsx --tsconfig examples/tsconfig.json examples/h2-multiplexing.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@changesets/cli": "catalog:",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "msw": "^2.12.3",
    "puppeteer": "^24.31.0",
    "tsup": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "catalog:"
  },
  "packageManager": "pnpm@10.17.1"
}
